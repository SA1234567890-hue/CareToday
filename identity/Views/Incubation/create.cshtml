@model identity.Models.Nursery
@{
    ViewData["Title"] = "index";
}

<form method="post">
    <h4>Create a new account.</h4>
    <hr />
    <div asp-validation-summary="All" class="text-danger"></div>
    <div class="form-group">
        @Html.LabelFor(n => n.NurCost)
        @Html.TextBoxFor(n => n.NurCost, new { @class = "form-control" })
    </div>
    <div class="form-group">
        @Html.LabelFor(n => n.NurPhone)
        @Html.TextBoxFor(n => n.NurPhone, new { @class = "form-control" })
    </div>
    <div class="form-group">
        @Html.LabelFor(n => n.NurLocation)
        @Html.TextBoxFor(n => n.NurLocation, new { @class = "form-control" })
    </div>
    <div class="form-group">
        @Html.LabelFor(n => n.NurDescription)
        @Html.TextBoxFor(n => n.NurDescription, new { @class = "form-control" })
    </div>

    <button type="submit" class="btn btn-primary">Save</button>
</form>
